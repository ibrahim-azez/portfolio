{
  "name": "porty",
  "version": "0.0.0",
  "contributors": [
    "Ibrahim Azez <ibrahimazez@protonmail.com>"
  ],
  "scripts": {
    "start:heroku": "node dist/porty/server",
    "heroku-postbuild": "npm run build:ssr  ",
    "vercel-build": "npm run build:ssr && npm run prerender",
    "optimizecss": "gulp --gulpfile build-scripts/optimize-css.gulpfile.js",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/browser/stats.json",
    "ng": "ng",
    "start": "ng serve --hmr",
    "build-dev": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "dev:ssr": "ng run porty:serve-ssr",
    "serve:ssr": "node dist/porty/server/main.js",
    "build:ssr": "ng build && ng run porty:server",
    "prerender": "ng run porty:prerender "
  },
  "private": true,
  "dependencies": {
