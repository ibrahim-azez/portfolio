{
  "name": "porty",
  "version": "0.0.0",
  "contributors": [
    "Ibrahim Azez <ibrahimazez@protonmail.com>"
  ],
  "scripts": {
    "start:heroku": "node dist/porty/server",
    "heroku-postbuild": "npm run build:ssr  ",
    "vercel-build": "npm run build:ssr && npm run prerender",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/browser/stats.json",
    "ng": "ng",
    "start": "ng serve --hmr",
    "build-dev": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "dev:ssr": "ng run porty:serve-ssr",
    "serve:ssr": "node dist/porty/server/main.js",
    "build:ssr": "ng build && ng run porty:server",
    "prerender": "ng run porty:prerender "
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.1",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/material": "^13.3.1",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/platform-server": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@angular/service-worker": "~13.3.0",
    "@nguniversal/express-engine": "^13.0.2",
    "bootstrap": "^5.1.3",
    "compression": "^1.7.4",
    "express": "^4.15.2",
    "rxjs": "~7.4.0",
    "source-map-explorer": "^2.5.2",
    "ts-evaluator": "^0.1.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.0",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "^13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@nguniversal/builders": "^13.0.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "eslint": "^8.2.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2",
    "webpack-bundle-analyzer": "^4.5.0"
  }
